{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.8.0"
  },
  "scripts": {
    "dev": "hexo server -p 4000",
    "preview": "hexo g && http-server -p 4001 public",
    "clean": "hexo clean",
    "list-post": "hexo list post",
    "build": "hexo g",
    "deploy": "hexo g -d -f && git add . && git commit -m 'update' && git push origin master && git push github master"
  },
  "dependencies": {
    "hexo": "^3.7.0",
    "hexo-codepen": "^0.1.1",
    "hexo-deployer-git": "^0.3.1",
    "hexo-generator-archive": "^0.1.5",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-feed": "^1.2.2",
    "hexo-generator-index": "^0.2.1",
    "hexo-generator-tag": "^0.2.0",
    "hexo-renderer-ejs": "^0.3.1",
    "hexo-renderer-markdown-it": "^3.4.1",
    "hexo-renderer-marked": "^0.3.2",
    "hexo-renderer-sass": "^0.3.2",
    "hexo-renderer-stylus": "^0.3.3",
    "hexo-server": "^0.3.1",
    "markdown-it-emoji": "^1.4.0",
    "twemoji": "^11.2.0"
  },
  "devDependencies": {
    "http-server": "^0.12.3"
  }
}
